# Deep Roots Operations Dashboard - Environment Variables
# Copy this file to .env.local and fill in your Google Apps Script deployment URLs

# Inventory API - Your clippings/inventory Google Apps Script URL
NEXT_PUBLIC_INVENTORY_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID_HERE/exec

# Grading Tool API - Your repair vs replace tool URL
NEXT_PUBLIC_GRADING_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID_HERE/exec

# Scheduler API - Your crew scheduler URL
NEXT_PUBLIC_SCHEDULER_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID_HERE/exec

# Tools Checkout API - Your hand tools checkout URL
NEXT_PUBLIC_TOOLS_API_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID_HERE/exec

# Chess Map - Your deployed Daily Chess Map URL
NEXT_PUBLIC_CHESSMAP_URL=https://dailychessmap.netlify.app
